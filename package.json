{
    "name": "fashionable-engineer",
    "version": "1.0.0",
    "description": "fashionable-engineer",
    "author": "hikarut",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "test": "jest"
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.9.0",
        "bulma": "^0.7.5",
        "nuxt": "^2.0.0"
    },
    "devDependencies": {
        "vue-jest": "^3.0.3",
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@vue/test-utils": "^1.0.0-beta.27",
        "@nuxtjs/style-resources": "^1.0.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^8.2.1",
        "babel-jest": "^24.1.0",
        "eslint": "^5.0.1",
        "eslint-config-prettier": "^3.1.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-prettier": "2.6.2",
        "eslint-plugin-vue": "^4.0.0",
        "jest": "^24.1.0",
        "json5-loader": "^2.0.0",
        "nodemon": "^1.18.9",
        "prettier": "1.14.3",
        "vue-lazyload": "^1.2.6",
        "sass-loader": "^7.3.1",
        "node-sass": "^4.12.0"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest",
            ".*\\.(vue)$": "vue-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1",
            "^@@/(.*)$": "<rootDir>/$1",
            "^~/(.*)$": "<rootDir>/$1",
            "^~~/(.*)$": "<rootDir>/$1"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{js,vue}",
            "!**/node_modules/**"
        ]
    },
    "babel": {
        "env": {
            "test": {
                "presets": [
                    "@babel/preset-env"
                ]
            }
        }
    }
}
